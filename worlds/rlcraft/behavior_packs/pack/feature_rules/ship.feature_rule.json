{
  "format_version": "1.13.0",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "ecbl_bs:ship.feature_rule",
      "places_feature": "ecbl_bs:ship.feature"
    },
    "conditions": {
      "placement_pass": "surface_pass",
      "minecraft:biome_filter": {
        "all_of": [
          {
            "any_of": [
              {
                "test": "has_biome_tag",
                "value": "overworld"
              },
              {
                "test": "has_biome_tag",
                "value": "overworld_generation"
              }
            ]
          }
        ],
        "any_of": [
          {
            "test": "has_biome_tag",
            "value": "ocean"
          }
        ]
      }
    },
    "distribution": {
      "iterations": 1.0,
      "scatter_chance": {
        "numerator": 1,
        "denominator": 1000
      },
      "x": 0,
      "y": "query.heightmap(variable.worldx, variable.worldz) - 4",
      "z": -16
    }
  }
}
