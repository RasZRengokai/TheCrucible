{
	"format_version": "1.21.60",
	"minecraft:entity": {
		"description": {
			"identifier": "ecbl_bs:imp",
			"spawn_category": "monster",
			"is_spawnable": true,
			"is_summonable": true,
			"properties": {
				"ecbl_bs:animation_imp": {
					"client_sync": true,
					"type": "enum",
					"values": [
						"in_ground",
						"start_flying",
						"flying",
						"death"
					],
					"default": "in_ground"
				}
			}
		},
		"component_groups": {
			"ecbl_bs:start_fly": {
				"minecraft:timer": {
					"time": 1,
					"looping": true,
					"time_down_event": {
						"event": "ecbl_bs:add_fly",
						"target": "self"
					}
				},
				"minecraft:movement": {
					"value": 0
				}
			},
			"ecbl_bs:ground": {
				"minecraft:timer": {
					"time": 15,
					"looping": true,
					"time_down_event": {
						"event": "ecbl_bs:random_imp",
						"target": "self"
					}
				},
				"minecraft:movement": {
					"value": 0.35
				},
				"minecraft:physics": {},
				"minecraft:movement.basic": {
					"max_turn": 20
				},
				"minecraft:jump.static": {},
				"minecraft:navigation.walk": {
					"can_path_over_water": true
				},
				"minecraft:attack": {
					"damage": 3
				},
				"minecraft:behavior.delayed_attack": {
					"attack_duration": 1.17,
					"attack_once": false,
					"attack_types": "",
					"hit_delay_pct": 0.33,
					"random_stop_interval": 0,
					"reach_multiplier": 2,
					"require_complete_path": true,
					"track_target": false
				}
			},
			"ecbl_bs:fly": {
				"minecraft:behavior.nearest_attackable_target": {
					"priority": 1,
					"set_persistent": true,
					"within_radius": 10,
					"must_see": true,
					"must_reach": true,
					"reselect_targets": true,
					"entity_types": [
						{
							"max_dist": 10,
							"filters": {
								"test": "is_family",
								"subject": "other",
								"value": "player"
							}
						}
					]
				},
				"minecraft:timer": {
					"time": 15,
					"looping": true,
					"time_down_event": {
						"event": "ecbl_bs:random_imp_fly",
						"target": "self"
					}
				},
				"minecraft:physics": {},
				"minecraft:flying_speed": {
					"value": 0.35
				},
				"minecraft:movement": {
					"value": 0.35
				},
				"minecraft:can_fly": {},
				"minecraft:movement.hover": {
					"max_turn": 20
				},
				"minecraft:jump.static": {},
				"minecraft:navigation.hover": {
					"can_path_from_air": true,
					"can_sink": false,
					"can_path_over_water": true,
					"avoid_water": true
				},
				"minecraft:behavior.random_hover": {
					"priority": 2,
					"xz_dist": 8,
					"y_dist": 2,
					"y_offset": -2,
					"interval": 1,
					"hover_height": [
						0,
						1
					]
				},
				"minecraft:attack": {
					"damage": 3
				},
				"minecraft:behavior.melee_attack": {
					"priority": 1
				}
			},
			"ecbl_bs:death": {
				"minecraft:movement": {
					"value": 0.0
				},
				"minecraft:pushable": {
					"is_pushable": false,
					"is_pushable_by_piston": false
				},
				"minecraft:damage_sensor": {
					"triggers": {
						"cause": "all",
						"deals_damage": "no"
					}
				},
				"minecraft:timer": {
					"time": 2,
					"time_down_event": {
						"event": "ecbl_bs:despawn_event"
					}
				},
				"minecraft:behavior.drop_item_for": {
					"seconds_before_pickup": 0.0,
					"cooldown": 0,
					"drop_item_chance": 1,
					"offering_distance": 0.0,
					"minimum_teleport_distance": 1024.0,
					"target_range": [
						64.0,
						64.0,
						64.0
					],
					"teleport_offset": [
						0.0,
						1.0,
						0.0
					],
					"speed_multiplier": 1.0,
					"search_range": 64,
					"search_height": 64,
					"search_count": 0,
					"goal_radius": 64.0,
					"entity_types": [
						{
							"filters": {
								"test": "is_family",
								"subject": "other",
								"value": "player"
							},
							"max_dist": 64
						}
					],
					"priority": 1,
					"loot_table": "loot_tables/eternal/better_structures/entities/imp.json",
					"time_of_day_range": [
						0.0,
						1.0
					]
				},
				"minecraft:physics": {}
			},
			"ecbl_bs:despawn": {
				"minecraft:instant_despawn": {},
				"minecraft:physics": {}
			}
		},
		"components": {
			"minecraft:behavior.melee_box_attack": {
				"melee_fov": 90,
				"track_target": false,
				"attack_types": "example",
				"random_stop_interval": 0,
				"attack_once": false,
				"require_complete_path": false,
				"cooldown_time": 0
			},
			"minecraft:behavior.melee_attack": {
				"melee_fov": 90,
				"track_target": false,
				"attack_types": "example",
				"random_stop_interval": 0,
				"reach_multiplier": 2,
				"attack_once": false,
				"require_complete_path": false,
				"cooldown_time": 0
			},
			"minecraft:behavior.nearest_attackable_target": {
				"priority": 2,
				"set_persistent": true,
				"within_radius": 10,
				"must_see": true,
				"must_reach": true,
				"reselect_targets": true,
				"entity_types": [
					{
						"max_dist": 10,
						"filters": {
							"test": "is_family",
							"subject": "other",
							"value": "player"
						}
					}
				]
			},
			"minecraft:damage_sensor": {
				"triggers": {
					"deals_damage": "no",
					"on_damage": {
						"filters": {
							"test": "has_damage",
							"subject": "self",
							"value": "fatal"
						},
						"event": "ecbl_bs:event_death_imp"
					}
				}
			},
			"minecraft:on_death": {
				"event": "ecbl_bs:event_death_imp"
			},
			"minecraft:attack": {
				"damage": 3
			},
			"minecraft:behavior.hurt_by_target": {
				"priority": 0
			},
			"minecraft:equip_item": {},
			"minecraft:is_hidden_when_invisible": {},
			"minecraft:experience_reward": {
				"on_death": "query.last_hit_by_player ? (query.is_baby ? 12 : 5) + (Math.die_roll(query.equipment_count,1,3)) : 0"
			},
			"minecraft:type_family": {
				"family": [
					"imp",
					"monster",
					"mob"
				]
			},
			"minecraft:breathable": {
				"total_supply": 15,
				"suffocate_time": 0
			},
			"minecraft:collision_box": {
				"width": 0.7,
				"height": 0.7
			},
			"minecraft:health": {
				"value": 16,
				"max": 16
			},
			"minecraft:jump.static": {},
			"minecraft:follow_range": {
				"value": 64
			},
			"minecraft:loot": {
				"table": "loot_tables/eternal/better_structures/entities/imp.json"
			},
			"minecraft:behavior.float": {
				"priority": 0
			},
			"minecraft:behavior.random_stroll": {
				"priority": 3,
				"speed_multiplier": 1
			},
			"minecraft:behavior.look_at_player": {
				"priority": 10,
				"look_distance": 8,
				"probability": 0.02
			},
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": true,
				"is_pushable_by_piston": true
			}
		},
		"events": {
			"minecraft:entity_spawned": {
				"add": {
					"component_groups": [
						"ecbl_bs:start_fly"
					]
				},
				"set_property": {
					"ecbl_bs:animation_imp": "start_flying"
				}
			},
			"ecbl_bs:random_imp": {
				"randomize": [
					{
						"add": {
							"component_groups": [
								"ecbl_bs:ground"
							]
						},
						"remove": {
							"component_groups": [
								"ecbl_bs:fly"
							]
						},
						"weight": 10,
						"set_property": {
							"ecbl_bs:animation_imp": "start_flying"
						}
					},
					{
						"remove": {
							"component_groups": [
								"ecbl_bs:ground"
							]
						},
						"add": {
							"component_groups": [
								"ecbl_bs:start_fly"
							]
						},
						"weight": 90,
						"set_property": {
							"ecbl_bs:animation_imp": "start_flying"
						}
					}
				]
			},
			"ecbl_bs:random_imp_fly": {
				"randomize": [
					{
						"add": {
							"component_groups": [
								"ecbl_bs:ground"
							]
						},
						"remove": {
							"component_groups": [
								"ecbl_bs:fly"
							]
						},
						"weight": 50,
						"set_property": {
							"ecbl_bs:animation_imp": "in_ground"
						}
					},
					{
						"weight": 50,
						"set_property": {
							"ecbl_bs:animation_imp": "flying"
						}
					}
				]
			},
			"ecbl_bs:remove_fly": {
				"add": {
					"component_groups": [
						"ecbl_bs:ground"
					]
				},
				"remove": {
					"component_groups": [
						"ecbl_bs:fly"
					]
				},
				"set_property": {
					"ecbl_bs:animation_imp": "in_ground"
				}
			},
			"ecbl_bs:add_fly": {
				"remove": {
					"component_groups": [
						"ecbl_bs:start_fly"
					]
				},
				"add": {
					"component_groups": [
						"ecbl_bs:fly"
					]
				},
				"set_property": {
					"ecbl_bs:animation_imp": "flying"
				}
			},
			"ecbl_bs:event_death_imp": {
				"set_property": {
					"ecbl_bs:animation_imp": "death"
				},
				"queue_command": {
					"command": "loot spawn ~~~ kill @s"
				},
				"remove": {
					"component_groups": [
						"ecbl_bs:start_fly",
						"ecbl_bs:ground",
						"ecbl_bs:fly"
					]
				},
				"add": {
					"component_groups": [
						"ecbl_bs:death"
					]
				}
			},
			"ecbl_bs:despawn_event": {
				"remove": {
					"component_groups": [
						"ecbl_bs:death"
					]
				},
				"add": {
					"component_groups": [
						"ecbl_bs:despawn"
					]
				}
			}
		}
	}
}