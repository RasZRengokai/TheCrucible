{
	"format_version": "1.14.0",
	"minecraft:entity": {
		"description": {
			"identifier": "jerrys_colonies:spectator",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false,
			
			"default_render_controller": "controller.render.citizen"
		},
		"component_groups": {
			"variant_0": { "minecraft:variant": { "value": 0 } },
			"variant_1": { "minecraft:variant": { "value": 1 } },
			"variant_2": { "minecraft:variant": { "value": 2 } },
			"variant_3": { "minecraft:variant": { "value": 3 } },
			"variant_4": { "minecraft:variant": { "value": 4 } },
			"variant_5": { "minecraft:variant": { "value": 5 } },
			"variant_6": { "minecraft:variant": { "value": 6 } },
			"variant_7": { "minecraft:variant": { "value": 7 } },
			"variant_8": { "minecraft:variant": { "value": 8 } },
			"variant_9": { "minecraft:variant": { "value": 9 } },
			"variant_10": { "minecraft:variant": { "value": 10 } },
			"variant_11": { "minecraft:variant": { "value": 11 } },
			"variant_12": { "minecraft:variant": { "value": 12 } },
			"variant_13": { "minecraft:variant": { "value": 13 } },
			"variant_14": { "minecraft:variant": { "value": 14 } },
			"variant_15": { "minecraft:variant": { "value": 15 } },
			"variant_16": { "minecraft:variant": { "value": 16 } },
			"variant_17": { "minecraft:variant": { "value": 17 } },
			"variant_18": { "minecraft:variant": { "value": 18 } },

		"random_anim_timer_5": { "minecraft:timer": { "time": 5, "time_down_event": { "event": "trigger_random_anim" } } },
		"random_anim_timer_6": { "minecraft:timer": { "time": 6, "time_down_event": { "event": "trigger_random_anim" } } },
		"random_anim_timer_7": { "minecraft:timer": { "time": 7, "time_down_event": { "event": "trigger_random_anim" } } },
		"random_anim_timer_8": { "minecraft:timer": { "time": 8, "time_down_event": { "event": "trigger_random_anim" } } },
		"random_anim_timer_9": { "minecraft:timer": { "time": 9, "time_down_event": { "event": "trigger_random_anim" } } },
		"random_anim_timer_10": { "minecraft:timer": { "time": 10, "time_down_event": { "event": "trigger_random_anim" } } },

		"anim_idle": {
			"minecraft:mark_variant": { "value": 0 }
		},

		"anim_active_1": {
				"minecraft:mark_variant": { "value": 1 },
				"minecraft:timer": { "time": 2, "time_down_event": { "event": "end_anim" } }
			},
			"anim_active_2": {
				"minecraft:mark_variant": { "value": 2 },
				"minecraft:timer": { "time": 2, "time_down_event": { "event": "end_anim" } }
			},
			"anim_active_3": {
				"minecraft:mark_variant": { "value": 3 },
				"minecraft:timer": { "time": 2, "time_down_event": { "event": "end_anim" } }
			},
			"anim_active_4": {
				"minecraft:mark_variant": { "value": 4 },
				"minecraft:timer": { "time": 2, "time_down_event": { "event": "end_anim" } }
			}
		},

		"events": {
			"minecraft:entity_spawned": {
				"randomize": [
					{ "weight": 1, "add": { "component_groups": [ "variant_0", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_1", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_2", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_3", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_4", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_5", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_6", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_7", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_8", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_9", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_10", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_11", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_12", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_13", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_14", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_15", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_16", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_17", "random_anim_timer_10" ] } },
					{ "weight": 1, "add": { "component_groups": [ "variant_18", "random_anim_timer_10" ] } }
				]
		},

		"trigger_random_anim": {
			"remove": { "component_groups": [ "anim_idle", "random_anim_timer_5", "random_anim_timer_6", "random_anim_timer_7", "random_anim_timer_8", "random_anim_timer_9", "random_anim_timer_10" ] },
			"randomize": [
				{ "weight": 1, "add": { "component_groups": [ "anim_active_1" ] } },
				{ "weight": 1, "add": { "component_groups": [ "anim_active_2" ] } },
				{ "weight": 1, "add": { "component_groups": [ "anim_active_3" ] } },
				{ "weight": 1, "add": { "component_groups": [ "anim_active_4" ] } }
			]
		},

		"end_anim": {
			"remove": { "component_groups": [ "anim_active_1", "anim_active_2", "anim_active_3", "anim_active_4" ] },
			"add": { "component_groups": [ "anim_idle" ] },
			"randomize": [
				{ "weight": 1, "add": { "component_groups": [ "random_anim_timer_5" ] } },
				{ "weight": 1, "add": { "component_groups": [ "random_anim_timer_6" ] } },
				{ "weight": 1, "add": { "component_groups": [ "random_anim_timer_7" ] } },
				{ "weight": 1, "add": { "component_groups": [ "random_anim_timer_8" ] } },
				{ "weight": 1, "add": { "component_groups": [ "random_anim_timer_9" ] } },
				{ "weight": 1, "add": { "component_groups": [ "random_anim_timer_10" ] } }
			]
		}
	},		"components": {
			"minecraft:type_family": { "family": [ "npc", "spectator", "mob" ] },
			"minecraft:health": { "value": 10000, "max": 10000 },
			"minecraft:collision_box": { "width": 0.6, "height": 1.675 },
			"minecraft:physics": {},
			"minecraft:pushable": { "is_pushable": false, "is_pushable_by_piston": false },
			"minecraft:breathable": { "total_supply": 10, "suffocate_time": 5, "breathes_air": true, "breathes_water": false, "generates_bubbles": true },
			"minecraft:damage_sensor": { "triggers": [ { "cause": "all", "deals_damage": "no" } ] },
			"minecraft:knockback_resistance": { "value": 1.0 }
		}
	}
}
