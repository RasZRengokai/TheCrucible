{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "ecbl_bs:witch_hut_fog",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/eternal/better_structures/particle/fog"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 50,
				"max_particles": 500
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 5
			},
			"minecraft:emitter_shape_disc": {
				"radius": 70,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.random(10, 15)"
			},
			"minecraft:particle_initial_spin": {
				"rotation": "variable.particle_random_2*360"
			},
			"minecraft:particle_initial_speed": 0.2,
			"minecraft:particle_motion_dynamic": {},
			"minecraft:particle_appearance_billboard": {
				"size": ["3+variable.particle_random_2", "3+variable.particle_random_2"],
				"facing_camera_mode": "rotate_xyz"
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#009DFC9F",
						"0.35": "#12A7FEA2",
						"1.0": "#0083FE8C"
					}
				}
			}
		}
	}
}