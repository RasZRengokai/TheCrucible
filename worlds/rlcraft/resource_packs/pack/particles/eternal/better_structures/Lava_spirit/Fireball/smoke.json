{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "ecbl_bs:smoke_2",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/eternal/better_structures/particle/smoke"
			}
		},
		"curves": {
			"variable.scale": {
				"type": "bezier_chain",
				"input": "v.particle_age/variable.particle_lifetime",
				"nodes": {
					"0.0": {
						"value": 0,
						"slope": 0.3
					},
					"1.0": {
						"value": 1,
						"slope": 0
					},
					"0.12": {
						"left_value": 1,
						"right_value": 1.64,
						"slope": -0.05
					},
					"0.14": {
						"value": 2.65,
						"slope": 0
					},
					"0.31": {
						"value": 2.46,
						"slope": -0.4
					}
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 50,
				"max_particles": 100
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 2
			},
			"minecraft:emitter_shape_sphere": {
				"radius": 0.5,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 1
			},
			"minecraft:particle_initial_spin": {
				"rotation": "variable.particle_random_2*math.random(360, -360)"
			},
			"minecraft:particle_initial_speed": 0.2,
			"minecraft:particle_motion_dynamic": {},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.3*variable.scale", "0.3*variable.scale"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 144,
					"texture_height": 16,
					"flipbook": {
						"base_UV": [16, 0],
						"size_UV": [16, 16],
						"step_UV": [16, 0],
						"max_frame": 9,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_radius": 0.01,
				"expire_on_contact": true
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "variable.particle_random_2",
					"gradient": {
						"0.0": "#FF989898",
						"0.15": "#FF959595",
						"0.43": "#FF858585",
						"1.0": "#FF9F9F9F"
					}
				}
			}
		}
	}
}