{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "ecbl_bs:elder_pillager_teleport",
			"basic_render_parameters": {
				"material": "particles_add",
				"texture": "textures/eternal/better_structures/particle/box"
			}
		},
		"curves": {
			"variable.scale": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [1.93, 1, 0.65, 0]
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 150,
				"max_particles": 20
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.3
			},
			"minecraft:emitter_shape_box": {
				"half_dimensions": [0.3, 0.6, 0.3],
				"surface_only": true,
				"direction": ["math.random(0.5, -0.5)", "math.random(0.5, -0.5)", "math.random(0.5, -0.5)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.random(1, 0.5)"
			},
			"minecraft:particle_initial_speed": "-10*variable.particle_random_2",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["math.cos(variable.particle_age*360)*math.random(5, -5)", "math.sin(variable.particle_age*360)", "math.sin(variable.particle_age*360)*math.random(5, -5)"],
				"linear_drag_coefficient": 5
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.08*variable.scale", "0.08*variable.scale"],
				"facing_camera_mode": "rotate_xyz"
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FF312DCE",
						"0.24": "#FF5B5FFD",
						"0.48": "#FF393BEE",
						"0.65": "#FF494CE2",
						"0.89": "#002F00FF",
						"1.0": "#00465EE8"
					}
				}
			}
		}
	}
}