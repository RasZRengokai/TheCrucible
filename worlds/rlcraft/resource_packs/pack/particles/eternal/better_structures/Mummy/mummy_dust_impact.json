			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.2, 0.2],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"uv": ["math.floor(variable.particle_random_2*5)*8", 0],
					"uv_size": [8, 8]
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_radius": 0.01
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "variable.particle_random_3 + v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#9E997D60",
						"0.21": "#91C9B096",
						"0.5": "#B3B99B7C",
						"1.0": "#00B99B7C"
