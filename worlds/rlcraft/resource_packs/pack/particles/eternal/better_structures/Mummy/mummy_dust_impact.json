{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "ecbl_bs:mummy_dust_impact",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particle/particles"
			}
		},
		"curves": {
			"variable.color": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0.79, 1, 0]
			},
			"variable.r": {
				"type": "linear",
				"input": "variable.particle_random_2",
				"horizontal_range": 1,
				"nodes": [1, 0.47, 1]
			},
			"variable.g": {
				"type": "linear",
				"input": "variable.particle_random_2",
				"horizontal_range": 1,
				"nodes": [1, 0.47, 1]
			},
			"variable.b": {
				"type": "linear",
				"input": "variable.particle_random_2",
				"horizontal_range": 1,
				"nodes": [1, 0.47, 1]
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 10
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.3
			},
			"minecraft:emitter_shape_box": {
				"half_dimensions": [0.4, 0.4, 0.4],
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 5
			},
			"minecraft:particle_initial_spin": {
