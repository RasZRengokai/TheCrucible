{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "ecbl_bs:negation_totem_impact",
			"basic_render_parameters": {
				"material": "particles_add",
				"texture": "textures/eternal/better_structures/particle/box"
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 30
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.3
			},
			"minecraft:emitter_shape_point": {
				"offset": [0, "Math.random(-0.9, -0.5)", 0],
				"direction": ["Math.random(-1.0, 1.0)", "Math.random(-1.0, 1.0)", "Math.random(-1.0, 1.0)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "3.0 + Math.random(0.0, 0.6)"
