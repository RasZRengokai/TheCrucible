{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "ecbl_bs:moth",
      "materials": {
        "default": "entity_alphatest"
      },
      "textures": {
        "default": "textures/eternal/better_structures/blocks/transparence"
      },
      "geometry": {
        "default": "geometry.ecbl_bs.particle"
      },
      "render_controllers": [
        "controller.render.default"
      ],
      "particle_effects": {
        "start": "ecbl_bs:moth_start",
        "exit": "ecbl_bs:moth_exit"
      },
      "animations": {
        "root": "controller.animation.ecbl_bs.particle_root",
        "start": "animation.ecbl_bs.particle_start",
        "exit": "animation.ecbl_bs.particle_exit"
      },
      "scripts": {
        "animate": [
          {
            "start": "q.property('ecbl_bs:particle') == 'start'"
          },
          {
            "exit": "q.property('ecbl_bs:particle') == 'exit'"
          }
        ]
      }
    }
  }
}