{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "jerrys_colonies:spectator",
			"materials": { "default": "entity_alphatest" },
			"textures": {
				"skin0": "textures/entity/skin0",
				"skin1": "textures/entity/skin1",
				"skin2": "textures/entity/skin2",
				"skin3": "textures/entity/skin3",
				"skin4": "textures/entity/skin4",
				"skin5": "textures/entity/skin5",
				"skin6": "textures/entity/skin6",
				"skin7": "textures/entity/skin7",
				"skin8": "textures/entity/skin8",
				"skin9": "textures/entity/skin9",
				"skin10": "textures/entity/skin10",
				"skin11": "textures/entity/skin11",
				"skin12": "textures/entity/skin12",
				"skin13": "textures/entity/skin13",
				"skin14": "textures/entity/skin14",
				"skin15": "textures/entity/skin15",
				"skin16": "textures/entity/skin16",
				"skin17": "textures/entity/skin17",
				"skin18": "textures/entity/skin18",
				"knight": "textures/entity/knight"
			},
			"geometry": { "default": "geometry.humanoid.custom" },
			"animations": {
				"idle": "animation.humanoid.citizen.idle",
				"walk_fast": "animation.citizen.idle_bob",
				"hold_item": "animation.humanoid.hold_item",
				"nap": "animation.citizen.nap",
				"wave": "animation.humanoid.citizen.wave",
				"fistshake": "animation.humanoid.citizen.fistshake",
				"dance": "animation.humanoid.citizen.dance",
				"shakehead": "animation.humanoid.citizen.shakehead",
				"movement": "controller.animation.citizen.movement"
			},
			"scripts": {
				"pre_animation": [ "variable.wave_timer = query.mark_variant;" ],
				"animate": [
					"movement",
					{ "idle": "variable.wave_timer == 0" },
					{ "wave": "variable.wave_timer == 1" },
					{ "fistshake": "variable.wave_timer == 2" },
					{ "dance": "variable.wave_timer == 3" },
					{ "shakehead": "variable.wave_timer == 4" }
				]
			},
			"render_controllers": [ "controller.render.citizen" ],
			"enable_attachables": true
		}
	}
}
